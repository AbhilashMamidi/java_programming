class VotingEligibility 
{
	public static void main(String[] args) 
	{
		int age = 20;
			String b = (age>=18)? "Eligible for voting":"Not Eligible for voting";
		System.out.println(b);
		int a = 20;
			String b = (a<=18)? "Not Eligible for voting":"Eligible for voting";
		System.out.println(b);
	}
}
